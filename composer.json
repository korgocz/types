{
  "name": "smartemailing/types",
  "description": "Lightweight collection of handy PHP value objects",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "strict types",
    "value objects"
  ],
  "authors": [
    {
      "name": "Martin Strouhal",
      "email": "martin@smartemailing.cz"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "consistence/consistence": "^1.0 || ^2.0",
    "nette/utils": "^3.0 || ^2.4",
    "nette/http": "^3.0 || ^2.4",
    "jschaedl/iban-validation": "^1.0",
    "egulias/email-validator": "^3.0"
  },
  "require-dev": {
    "nette/tester": "^2.3.0",
    "phpstan/phpstan-nette": "^0.12.8",
    "slevomat/coding-standard": "^6.4",
    "consistence/coding-standard": "^3.10",
    "tracy/tracy": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "SmartEmailing\\Types\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmartEmailing\\Types\\": [
        "tests"
      ]
    }
  }
}
